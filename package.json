{
  "name": "@oyai32/counter",
  "version": "1.0.1",
  "description": "xx组件",
  "type": "module",
  "main": "./dist/counter.umd.cjs",
  "module": "./dist/counter.js",
  "exports": {
    ".": {
      "import": "./dist/counter.js",
      "require": "./dist/counter.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run api:generate && npm run changelog:generate && vitepress dev docs",
    "build": "npm run api:generate && npm run changelog:generate && vitepress build docs",
    "preview": "vitepress preview docs",
    "lib:build": "vite build",
    "lib:publish": "npm version patch -m 'chore: release %s' && git push --follow-tags && npm run build:lib && npm publish",
    "demo:dev": "vite --open /demo/",
    "demo:preview": "vite preview --open /demo/",
    "api:generate": "node scripts/generate-api-doc.js",
    "changelog:generate": "node scripts/generate-changelog.js"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "conventional-changelog": "^7.0.0",
    "conventional-changelog-cli": "^2.2.0",
    "shiki": "^1.29.2",
    "typescript": "^5.0.0",
    "vite": "^7.1.11",
    "vitepress": "^1.0.0",
    "vue": "^3.3.0",
    "vue-docgen-api": "^4.79.2",
    "vue-docgen-cli": "^4.79.0",
    "vue-tsc": "^1.8.0"
  },
  "peerDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
